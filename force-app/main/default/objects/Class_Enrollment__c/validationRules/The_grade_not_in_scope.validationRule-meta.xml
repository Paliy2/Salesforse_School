<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>The_grade_not_in_scope</fullName>
    <active>true</active>
    <description>The grade should be in scope 1-100</description>
    <errorConditionFormula>OR (
Grade__c&gt;100,
Grade__c&lt;1
)</errorConditionFormula>
    <errorDisplayField>Class__c</errorDisplayField>
    <errorMessage>Error Location: Field Grade.</errorMessage>
</ValidationRule>
